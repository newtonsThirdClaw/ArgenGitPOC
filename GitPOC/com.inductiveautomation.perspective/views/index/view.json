        },
        "type": "ia.display.label"
      },
      {
        "custom": {
          "hi": 50,
          "hiColor": "#F78C00",
          "hihi": 75,
          "hihiColor": "#FF0000",
          "normColor": "#77B6D8"
        },
        "meta": {
          "name": "SimpleGauge"
        },
        "position": {
          "basis": "175px"
        },
        "propConfig": {
          "props.arc.color": {
            "binding": {
              "config": {
                "path": "this.props.value"
              },
              "transforms": [
                {
                  "code": "\tnewColor \u003d self.custom.normColor\n\thiColor \u003d self.custom.hiColor\n\thihiColor \u003d self.custom.hihiColor\n\thi \u003d self.custom.hi\n\thihi \u003d self.custom.hihi\n\t\n\tif value \u003e hihi:\n\t\tnewColor \u003d hihiColor\n\telif value \u003e hi:\n\t\tnewColor \u003d hiColor\n\t\n\treturn newColor",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "expression": "getSecond(now(2000))"
              },
              "transforms": [
                {
                  "code": "\timport random\n\tnewVal \u003d random.randint(0,90)\n\treturn newVal",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "arc": {},
          "style": {
            "classes": []
          }
        },
        "type": "ia.chart.simple-gauge"
